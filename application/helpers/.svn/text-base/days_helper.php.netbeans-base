<?php

/*
 * To change this template, choose Tools | Templates
 * and open the template in the editor.
 */


if(!function_exists("getDays"))
{
    function getDays()
    {
        $days = array();
        for($i=1;$i<31;$i++)
        {
            $days[$i] = $i;
        }

        
       return $days;
        
    }
    
}
?>
